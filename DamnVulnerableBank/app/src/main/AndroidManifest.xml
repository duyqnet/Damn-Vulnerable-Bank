<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.damnvulnerablebank">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".mybenifview"></activity>
        <activity android:name=".approvebenificiary" />
        <activity android:name=".pendingbenificiary" />
        <activity android:name=".addbenif" />
        <activity android:name=".sendmoney">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".viewbenif" />
        </activity>
        <activity android:name=".viewbenif" />
        <activity android:name=".transactions" />
        <activity android:name=".balanceview" />
        <activity android:name=".passcode" />
        <activity android:name=".Dashboard" />
        <activity android:name=".signup" />
        <activity android:name=".login" />
        <activity android:name=".banklogin" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>